<style>
  .et-test > div {
    margin: 15px;
    padding: 10px;
    outline: solid 2px dodgerblue;
  }
  .et-test > div::before {
    content: attr(class);
    font-weight: bold;
    display: block;
    font-style: italic;
    border-bottom: solid 1px dodgerblue;
    margin-bottom: 10px;
    padding-bottom: 5px;
    color: dodgerblue;
  }
</style>
<div class="et-test">
  {# @todo check current theme #}
  {#<div class="et-block">{{ get_block('bartik_main_menu') }}</div>#}
  <div class="et-block-plugin">{{ get_block_plugin('system_breadcrumb_block', {}) }}</div>
  {# @todo block content example generated by the module #}
  {#<div class="tt-block-content">{{ get_block_content(1) }}</div>#}
  <div class="et-entity-default">{{ get_entity('node', 1) }}</div>
  <div class="et-entity-teaser">{{ get_entity('node', 1, 'teaser') }}</div>
  <div class="et-entity-from-url">{{ get_entity('node') }}</div>
  <div class="et-field">{{ get_field('body', 'node', 1) }}</div>
  <div class="et-menu-default">{{ get_menu('main') }}</div>
  <div class="et-menu-level">{{ get_menu('main', 2) }}</div>
  <div class="et-menu-depth">{{ get_menu('main', 1, 1) }}</div>
  <div class="et-form">{{ get_form('Drupal\\system\\Form\\CronForm') }}</div>
  <div class="et-token">{{ get_token('site:name') }}</div>
  <div class="et-token-data">{{ get_token('node:title', {'node': node}) }}</div>
  <div class="et-config">{{ get_config('user.settings', 'anonymous') }}</div>

  {# @todo get url should negotiate route, path, internal, external #}
  <div class="et-url">{{ get_url('node/1', {absolute: true}) }}</div>
  {# provide core comparison #}
  <div class="et-twig-path">{{ path('entity.node.canonical', {'node': 1}) }}</div>
